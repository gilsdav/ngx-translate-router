{
  "name": "ngx-translate-router-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:main && npm run build:http-loader",
    "build:main": "ng build --prod ngx-translate-router",
    "build:http-loader": "ng build --prod ngx-translate-router-http-loader",
    "build:scully-plugin": "mkdir dist/ngx-translate-router-scully-plugin/ && cp ./projects/ngx-translate-router-scully-plugin/package.json dist/ngx-translate-router-scully-plugin/ && tsc ./projects/ngx-translate-router-scully-plugin/src/lib/scully-plugin.ts -d --outDir dist/ngx-translate-router-scully-plugin/",
    "test": "npm run test:main -- --watch=false --browsers=ChromeHeadless && npm run test:http-loader -- --watch=false --browsers=ChromeHeadless",
    "test:main": "ng test ngx-translate-router",
    "test:http-loader": "ng test ngx-translate-router-http-loader",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@scullyio/scully": "0.0.102",
    "rxjs": "~6.5.5",
    "tslib": "^1.13.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^9.1.9",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/language-service": "^9.1.9",
    "ng-packagr": "^9.1.5",
    "@types/node": "^14.0.5",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3"
  }
}
