{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@gilsdav/ngx-translate-router": [
        "dist/ngx-translate-router"
      ],
      "@gilsdav/ngx-translate-router/*": [
        "dist/ngx-translate-router/*"
      ],
      "@gilsdav/ngx-translate-router-http-loader": [
        "dist/ngx-translate-router-http-loader"
      ],
      "@gilsdav/ngx-translate-router-http-loader/*": [
        "dist/ngx-translate-router-http-loader/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
